class Solution {
    public int findLucky(int[] arr) {
        int[] cnt = new int[501];
        for (int a : arr) {
            ++cnt[a];
        }
        for (int i = 500; i > 0; --i) {
            if (cnt[i] == i) {
                return i;
            }
        }
        return -1;
    }
}

class Solution:
    def findLucky(self, arr: List[int]) -> int:
        ans = [-1]
        for i in set(arr):
            if arr.count(i) == i:
                ans.append(i)		
        return max(ans)
